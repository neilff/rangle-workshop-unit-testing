    "tdd:03": "mocha --compilers js:babel-core/register ./exercises/03-saving-time-no-tests/**/*.test.js -w",
    "tdd:04": "mocha --compilers js:babel-core/register ./exercises/04-tests-for-better-code/**/*.test.js -w"
  },
  "dependencies": {
    "ramda": "^0.22.1"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "mocha": "^3.0.2",
    "nyc": "^7.1.0"
